	return(1) if /(Invalid input detected|Type help or )/;
	return(-1) if (/command authorization failed/i);

	ProcessHistory("COMMENTS","keysort","K1","!CORES: $_");
	$lines++;
    }
    if ($lines) {
	ProcessHistory("COMMENTS","keysort","K0","!\n");
    }
    return(0);
}

